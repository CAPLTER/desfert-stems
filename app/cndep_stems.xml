          <item>
            <itextId>static_instance-treatment-3</itextId>
            <name>N+P</name>
          </item>
        </root>
      </instance>
      <instance id="yes_no">
        <root>
          <item>
            <itextId>static_instance-yes_no-0</itextId>
            <name>yes</name>
          </item>
          <item>
            <itextId>static_instance-yes_no-1</itextId>
            <name>no</name>
          </item>
        </root>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/cndep_stems/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/cndep_stems/end" type="dateTime"/>
      <bind jr:preload="date" jr:preloadParams="today" nodeset="/cndep_stems/today" type="date"/>
      <bind nodeset="/cndep_stems/site" required="true()" type="select1"/>
      <bind nodeset="/cndep_stems/plot_id" required="true()" type="select1"/>
      <bind nodeset="/cndep_stems/treatment" type="select1"/>
      <bind nodeset="/cndep_stems/survey_date" required="true()" type="date"/>
      <bind nodeset="/cndep_stems/plot_notes" type="string"/>
      <bind nodeset="/cndep_stems/shrub/plant_id" required="true()" type="select1"/>
      <bind nodeset="/cndep_stems/shrub/plant_note" type="string"/>
      <bind nodeset="/cndep_stems/shrub/old/old_stems/old_direction" required="true()" type="select1"/>
      <bind nodeset="/cndep_stems/shrub/old/old_stems/oldLength" type="int"/>
      <bind nodeset="/cndep_stems/shrub/old/allStems" type="select1"/>
      <bind nodeset="/cndep_stems/shrub/old/old_stems_missing" relevant=" /cndep_stems/shrub/old/allStems ='no'"/>
      <bind constraint="not(selected(., 'not_found') and selected(., 'dead'))" nodeset="/cndep_stems/shrub/old/old_stems_missing/north_missing" type="select"/>
      <bind constraint="not(selected(., 'not_found') and selected(., 'dead'))" nodeset="/cndep_stems/shrub/old/old_stems_missing/south_missing" type="select"/>
      <bind constraint="not(selected(., 'not_found') and selected(., 'dead'))" nodeset="/cndep_stems/shrub/old/old_stems_missing/west_missing" type="select"/>
      <bind constraint="not(selected(., 'not_found') and selected(., 'dead'))" nodeset="/cndep_stems/shrub/old/old_stems_missing/east_missing" type="select"/>
      <bind nodeset="/cndep_stems/shrub/new/new_stems/new_direction" required="true()" type="select1"/>
      <bind nodeset="/cndep_stems/shrub/new/new_stems/newLength" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/cndep_stems/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <select1 ref="/cndep_stems/site">
      <label>site</label>
      <item>
        <label>DBG</label>
        <value>DBG</value>
      </item>
      <item>
        <label>EME</label>
        <value>EME</value>
      </item>
      <item>
        <label>EMW</label>
        <value>EMW</value>
      </item>
      <item>
        <label>LDP</label>
        <value>LDP</value>
      </item>
      <item>
        <label>MCN</label>
        <value>MCN</value>
      </item>
      <item>
        <label>MCS</label>
        <value>MCS</value>
      </item>
      <item>
        <label>MVP</label>
        <value>MVP</value>
      </item>
      <item>
        <label>PWP</label>
        <value>PWP</value>
      </item>
      <item>
        <label>SME</label>
        <value>SME</value>
      </item>
      <item>
        <label>SMW</label>
        <value>SMW</value>
      </item>
      <item>
        <label>SNE</label>
        <value>SNE</value>
      </item>
      <item>
        <label>SNW</label>
        <value>SNW</value>
      </item>
      <item>
        <label>SRR</label>
        <value>SRR</value>
      </item>
      <item>
        <label>UMP</label>
        <value>UMP</value>
      </item>
      <item>
        <label>WTM</label>
        <value>WTM</value>
      </item>
    </select1>
    <select1 ref="/cndep_stems/plot_id">
      <label>plod ID</label>
      <itemset nodeset="instance('plot_id')/root/item[site= /cndep_stems/site ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <select1 ref="/cndep_stems/treatment">
      <label>treatment</label>
      <item>
        <label>Control</label>
        <value>C1</value>
      </item>
      <item>
        <label>N</label>
        <value>N</value>
      </item>
      <item>
        <label>P</label>
        <value>P</value>
      </item>
      <item>
        <label>N+P</label>
        <value>N+P</value>
      </item>
    </select1>
    <input ref="/cndep_stems/survey_date">
      <label>date</label>
    </input>
    <input ref="/cndep_stems/plot_notes">
      <label>note about plot</label>
    </input>
    <group ref="/cndep_stems/shrub">
      <label>concat('shrub_', <output value=" /cndep_stems/shrub/plant_id "/>)</label>
      <repeat nodeset="/cndep_stems/shrub">
        <select1 ref="/cndep_stems/shrub/plant_id">
          <label>plant</label>
          <item>
            <label>L1</label>
            <value>L1</value>
          </item>
          <item>
            <label>L2</label>
            <value>L2</value>
          </item>
          <item>
            <label>L3</label>
            <value>L3</value>
          </item>
          <item>
            <label>L4</label>
            <value>L4</value>
          </item>
          <item>
            <label>L5</label>
            <value>L5</value>
          </item>
        </select1>
        <input ref="/cndep_stems/shrub/plant_note">
          <label>note about plant</label>
        </input>
        <group ref="/cndep_stems/shrub/old">
          <label>old</label>
          <group ref="/cndep_stems/shrub/old/old_stems">
            <label>old_stems</label>
            <repeat nodeset="/cndep_stems/shrub/old/old_stems">
              <select1 ref="/cndep_stems/shrub/old/old_stems/old_direction">
                <label>direction</label>
                <item>
                  <label>N</label>
                  <value>N</value>
                </item>
                <item>
                  <label>S</label>
                  <value>S</value>
                </item>
                <item>
                  <label>W</label>
                  <value>W</value>
                </item>
                <item>
                  <label>E</label>
                  <value>E</value>
                </item>
              </select1>
              <input ref="/cndep_stems/shrub/old/old_stems/oldLength">
                <label>old length</label>
              </input>
            </repeat>
          </group>
          <select1 ref="/cndep_stems/shrub/old/allStems">
            <label>were all stems found?</label>
            <item>
              <label>yes</label>
              <value>yes</value>
            </item>
            <item>
              <label>no</label>
              <value>no</value>
            </item>
          </select1>
          <group appearance="field-list" ref="/cndep_stems/shrub/old/old_stems_missing">
            <label>stems_missing</label>
            <select ref="/cndep_stems/shrub/old/old_stems_missing/north_missing">
              <label>north</label>
              <item>
                <label>not found</label>
                <value>not_found</value>
              </item>
              <item>
                <label>dead (on branch or ground)</label>
                <value>dead</value>
              </item>
            </select>
            <select ref="/cndep_stems/shrub/old/old_stems_missing/south_missing">
              <label>south</label>
              <item>
                <label>not found</label>
                <value>not_found</value>
              </item>
              <item>
                <label>dead (on branch or ground)</label>
                <value>dead</value>
              </item>
            </select>
            <select ref="/cndep_stems/shrub/old/old_stems_missing/west_missing">
              <label>west</label>
              <item>
                <label>not found</label>
                <value>not_found</value>
              </item>
              <item>
                <label>dead (on branch or ground)</label>
                <value>dead</value>
              </item>
            </select>
            <select ref="/cndep_stems/shrub/old/old_stems_missing/east_missing">
              <label>east</label>
              <item>
                <label>not found</label>
                <value>not_found</value>
              </item>
              <item>
                <label>dead (on branch or ground)</label>
                <value>dead</value>
              </item>
            </select>
          </group>
        </group>
        <group ref="/cndep_stems/shrub/new">
          <label>new</label>
          <group ref="/cndep_stems/shrub/new/new_stems">
            <label>new_stems</label>
            <repeat nodeset="/cndep_stems/shrub/new/new_stems">
              <select1 ref="/cndep_stems/shrub/new/new_stems/new_direction">
                <label>direction</label>
                <item>
                  <label>N</label>
                  <value>N</value>
                </item>
                <item>
                  <label>S</label>
                  <value>S</value>
                </item>
                <item>
                  <label>W</label>
                  <value>W</value>
                </item>
                <item>
                  <label>E</label>
                  <value>E</value>
                </item>
              </select1>
              <input ref="/cndep_stems/shrub/new/new_stems/newLength">
                <label>new length</label>
              </input>
            </repeat>
          </group>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
